{
  "expo": {
    "name": "Intervals",
    "slug": "intervals",
    "version": "5.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "intervals",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 5000,
      "url": "https://u.expo.dev/2b6d1444-cdad-44b6-82b5-58ca17788482"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "4",
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.mrarich.Intervals",
      "associatedDomains": [
        "applinks:mrarich.com"
      ],
      "infoPlist": {
        "NSUserTrackingUsageDescription": "We'd like to show relevant ads.",
        "NSMicrophoneUsageDescription": "We'll play alerts for your intervals."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "com.google.android.gms.permission.AD_ID"
      ],
      "package": "com.mrarich.Intervals",
      "versionCode": 9
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "build": {
        "babel": {
          "include": [
            "@ui-kitten/components",
            "@ui-kitten/eva-icons"
          ]
        }
      }
    },
    "extra": {
      "MyVersion": "33",
      "eas": {
        "projectId": "2b6d1444-cdad-44b6-82b5-58ca17788482"
      }
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "organization": "alex-rich",
          "project": "intervals"
        }
      ],
      [
        "expo-tracking-transparency",
        {
          "userTrackingPermission": "We'd like to show relevant ads."
        }
      ],
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}